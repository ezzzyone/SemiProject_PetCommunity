<style>:host {
  box-sizing: border-box;
  font-family: var(--font, "Centra No1", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif); }

*,
::before,
::after {
  box-sizing: inherit; }

a {
  color: inherit;
  text-decoration: none; }

p {
  color: var(--detail-text-color, #465280);
  font-size: var(--detail-font-size, 0.875rem);
  font-weight: normal;
  line-height: 1.3em; }

.marquee-tile {
  flex-direction: var(--card-flex-direction, row);
  margin: 0;
  align-items: stretch;
  background-color: var(--color-white, #fff);
  border-radius: var(--card-border-radius, var(--corner-radius, 0.35rem));
  display: flex;
  width: auto;
  border: 1px solid var(--card-border-color, #CCCFDB);
  height: var(--tile-height, 316px);
  overflow: hidden; }
  .marquee-tile figure {
    background-position: center center;
    background-size: cover;
    flex: var(--figure-flex, 1 1 80px);
    margin: 0;
    object-fit: contain;
    padding: var(--card-padding, 0.75rem);
    min-height: 100px;
    height: 100%; }
  .marquee-tile .sponsored-text {
    background: var(--pill-bg-color, var(--color-white, #fff));
    border-radius: var(--pill-corner-radius, 2500rem);
    color: var(--pill-txt-color, #465280);
    font-size: var(--pill-font-size, small);
    font-weight: var(--pill-font-wt, 500);
    min-height: var(--pill-min-height, 1.5rem);
    min-width: var(--pill-min-width, 1.5rem);
    padding: 0.085rem 0.5rem 0.075rem 0.5rem;
    position: relative;
    top: 12px; }
  .marquee-tile .content {
    display: flex;
    flex: var(--content-flex, 1 1 66.6%);
    flex-direction: var(--content-direction, row);
    padding: var(--card-padding, 0.75rem);
    justify-content: var(--content-justification, unset);
    margin-top: auto;
    margin-bottom: auto; }
  .marquee-tile .logo {
    flex-direction: column;
    display: var(--logo-display, flex);
    align-self: center;
    max-height: 50px;
    padding-right: var(--spacing-three, 12px); }
  @media screen and (max-width: 525px) {
    .marquee-tile .logo {
      align-self: start; } }
  .marquee-tile .heading {
    color: var(--header-text-color, #465280);
    font-size: var(--header-font-size, 1.375rem);
    font-weight: var(--header-font-weight, 700);
    line-height: var(--header-line-height, 1.3em);
    margin: 0; }
  .marquee-tile .details {
    flex: 0 1 auto;
    font-size: var(--detail-font-size, 0.875rem);
    justify-content: center; }
    .marquee-tile .details .offer-text {
      font-weight: var(--detail-font-weight, 400);
      font-size: var(--detail-font-size, 0.875rem);
      line-height: var(--detail-line-height, calc(18 / 14));
      margin-block-start: var(--spacing-two, 8px);
      margin-block-end: var(--spacing-two, 8px);
      height: auto;
      overflow: hidden;
