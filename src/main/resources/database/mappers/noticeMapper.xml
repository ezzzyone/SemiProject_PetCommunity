<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pet.home.board.notice.NoticeDAO">
	<select id="getList" resultType="NoticeDTO">
		SELECT * FROM NOTICE
	</select>
	
	<insert id="setAdd" parameterType="NoticeDTO">
		INSERT INTO NOTICE(NUM, TITLE, WRITER, REGDATE, HIT, CONTENTS)
		VALUES(NOTICE_SEQ.NEXTVAL, #{title}, #{writer}, SYSDATE, 0, #{contents})
	</insert>
</mapper>